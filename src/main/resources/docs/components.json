      "category": "data",
      "description": "Tabla interactiva con filtros dependientes, paginación, búsqueda y ordenamiento. Integración nativa con Google Sheets.",
      "keywords": ["tabla", "filtros", "paginación", "búsqueda", "google sheets", "datatable", "grilla", "listado", "datos", "ordenar", "filtrar"],
      "dependencies": {
        "css": [
          "https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css",
          "/profiles/argentinagobar/themes/contrib/poncho/css/poncho.min.css",
          "/profiles/argentinagobar/themes/contrib/poncho/css/ponchoTable-1.1.css"
        ],
        "js": [
          "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js",
          "https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js",
          "/profiles/argentinagobar/themes/contrib/poncho/js/datatables.min.js",
          "/profiles/argentinagobar/themes/contrib/poncho/js/poncho.min.js"
        ]
      },
      "options": [
        {"name": "jsonUrl", "type": "string", "required": true, "description": "URL del documento o servicio JSON con los datos"},
        {"name": "tituloTabla", "type": "string", "required": false, "description": "Título para la etiqueta caption de la tabla"},
        {"name": "ordenColumna", "type": "integer", "required": false, "description": "Columna por la cual ordenar (índice numérico)"},
        {"name": "ordenTipo", "type": "string", "required": false, "default": "asc", "description": "Orden: 'asc' o 'desc'"},
        {"name": "ocultarColumnas", "type": "array", "required": false, "description": "Lista de columnas a ocultar"},
        {"name": "cantidadItems", "type": "integer", "required": false, "default": 10, "description": "Registros por página"},
        {"name": "filterClassList", "type": "array", "required": false, "description": "Clases CSS para cada columna de filtros"},
        {"name": "allowedTags", "type": "array", "required": false, "description": "Etiquetas HTML permitidas. ['*'] para todas"},
        {"name": "orderFilter", "type": "boolean|array", "required": false, "description": "Ordenar filtros alfabéticamente"},
        {"name": "wizard", "type": "boolean", "required": false, "default": false, "description": "Modo wizard para filtros dependientes"},
        {"name": "urlParams", "type": "boolean", "required": false, "default": false, "description": "Habilitar parámetros de URL"},
        {"name": "pushState", "type": "boolean", "required": false, "default": false, "description": "Actualizar URL con filtros"},
        {"name": "copyResults", "type": "boolean", "required": false, "default": false, "description": "Botón para copiar URL de resultados"},
        {"name": "headers", "type": "object", "required": false, "description": "Redefinir headers de columnas"},
        {"name": "customEntry", "type": "function", "required": false, "description": "Función para modificar cada entrada"},
        {"name": "refactorEntries", "type": "function", "required": false, "description": "Función para modificar el JSON completo"}
